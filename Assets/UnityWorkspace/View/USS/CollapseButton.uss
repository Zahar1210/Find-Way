UWCollapseButton {
    transition-duration: 0.2s;
    width: 20px;
    height: 20px;
    background-image: url('project://database/Assets/UnityWorkspace/Sprites/arrow-right@2x.texture2D?fileID=2800000&guid=f4974950146c541f081cf0605ad39271&type=2#arrow-right@2x');
    rotate: 90deg;
}

UWCollapseButton.collapsed {
    rotate: 0;
    -unity-background-image-tint-color: rgb(204, 204, 204);
}

UWCollapseButton:hover {
    scale: 1.2 1.2;
    -unity-background-image-tint-color: rgb(128, 128, 128);
}
